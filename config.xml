        <variable name="APP_KEY" value="mm1996dc5232cb4e28" />
        <variable name="APP_SECRET" value="c01ccafe718846cdbddf49f1fcceb7d1" />
    </plugin>
    <engine name="android" spec="^7.1.4" />
